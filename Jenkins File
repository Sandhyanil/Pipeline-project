pipeline {  
    agent any  
    stages {  
            stage ('Git-Checkout') {  
                steps{
                    git url: 'https://github.com/Sandhyanil/Pipeline-project.git'
                    echo "Checkout successful";
                } 
            }
            stage ('Compile') {  
                  steps{
                    bat label: '', script: 'mvnw compile'
                    echo "test successful"; 
                    
                } 
            }
            stage ('Build') {  
                  steps{
                    bat label: '', script: 'mvnw clean'
                    bat label: '', script: 'mvnw package'
                    echo "build successful";
                    
                } 
            }
             stage ('Test') {  
                  steps{
                    bat label: '', script: 'mvnw test'
                    echo "test successful";
                    
                } 
            }
           stage ('deploy') {  
                  steps{
                    copy "C:\Users\parsh\Documents\workspace-spring-tool-suite-4-4.12.1.RELEASE\Newproj\target, /ec2-user/java/apache-tomcat-8.5.73/webapps"
                
    }
}
